max_epochs: 1 
test_epoch: 1
lr_policy: "poly"
learning_rate: 0.05
power: 2
momentum: 0.9
weight_decay: 0.0002
warmup_lr: 0.05
max_iter: 2400000
type: "SGD"
global: true
load_checkpoint: false
checkpoint_dir: "./weights"
checkpoint_format: "binary"
